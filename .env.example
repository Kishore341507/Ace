# Discord Bot Configuration
# Copy this file to .env and fill in your actual values

# Your Discord bot token from https://discord.com/developers/applications
TOKEN="YOUR_DISCORD_BOT_TOKEN_HERE"

# PostgreSQL database connection string
# Format: postgresql://username:password@host:port/database
DB="postgresql://username:password@localhost:5432/ace_bot"

# Optional: Discord channel ID for error logging
# Leave empty or remove if you don't want error logging
ERROR_CHANNEL_ID=""

# Optional: Discord channel ID for bug reports
# Leave empty or remove if you don't want bug reporting feature
BUG_REPORT_CHANNEL_ID=""
